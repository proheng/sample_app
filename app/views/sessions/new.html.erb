<h1>Sign in</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_for :session, :url => sessions_path do |f| %>
  <div class="field">
  	<%= f.label :email %><br/>
  	<%= f.text_field :email %>
  </div>
  <div class="field">
  	<%= f.label :password %><br/>
  	<%= f.text_field :password %>
  </div>
  <div class="actions">
  	<%= f.submit :value=>"Sign In Now", :name => "submit" %>
  </div>
<% end %>
<p> <%= link_to  "New User?", signup_path %> </p>